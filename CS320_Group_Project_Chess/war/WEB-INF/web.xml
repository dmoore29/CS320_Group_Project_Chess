<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets --><servlet>
  	<servlet-name>MultiplyNumbersServlet</servlet-name>
  	<servlet-class>edu.ycp.cs320.Group_Project_Chess.servlet.MultiplyNumbersServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>IndexServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.Group_Project_Chess.servlet.IndexServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddNumbersServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.Group_Project_Chess.servlet.AddNumbersServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GuessingGameServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.Group_Project_Chess.servlet.GuessingGameServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ChessHomeServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.Group_Project_Chess.servlet.ChessHomeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ChessHomeServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.Group_Project_Chess.servlet.ChessHomeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<servlet-class>edu.ycp.cs320.Group_Project_Chess.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HomeServlet</servlet-name>
  	<servlet-class>edu.ycp.cs320.Group_Project_Chess.servlet.HomeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ChessGameServlet</servlet-name>
  	<servlet-class>edu.ycp.cs320.Group_Project_Chess.servlet.ChessGameServlet</servlet-class>
  </servlet>

  <!-- AJAX servlets. -->
  <servlet>
  	<servlet-name>MultiplyNumbersAjaxServlet</servlet-name>
  	<servlet-class>edu.ycp.cs320.Group_Project_Chess.servlet.ajax.MultiplyNumbersAjaxServlet</servlet-class>
  </servlet><servlet>
    <servlet-name>AddNumbersAjaxServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.Group_Project_Chess.servlet.ajax.AddNumbersAjaxServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>NextGuessAjaxServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.Group_Project_Chess.servlet.ajax.NextGuessAjaxServlet</servlet-class>
  </servlet>

  <!-- Servlet mappings -->
  <servlet-mapping>
  	<servlet-name>MultiplyNumbersServlet</servlet-name>
  	<url-pattern>/multiplyNumbers</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ChessHomeServlet</servlet-name>
    <url-pattern>/chessHome</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddNumbersServlet</servlet-name>
    <url-pattern>/addNumbers</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GuessingGameServlet</servlet-name>
    <url-pattern>/guessingGame</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HomeServlet</servlet-name>
  	<url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ChessGame</servlet-name>
  	<url-pattern>/chessGame</url-pattern>
  </servlet-mapping>

  <!--  AJAX servlet mappings. -->
  <servlet-mapping>
  	<servlet-name>MultiplyNumbersAjaxServlet</servlet-name>
  	<url-pattern>/ajax/multiplyNumbers</url-pattern>
  </servlet-mapping><servlet-mapping>
    <servlet-name>AddNumbersAjaxServlet</servlet-name>
    <url-pattern>/ajax/addNumbers</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>NextGuessAjaxServlet</servlet-name>
    <url-pattern>/ajax/nextGuess</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
</web-app>
